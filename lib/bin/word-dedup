#!/usr/bin/env python3

import re
import sys

from itertools import zip_longest


def get(xs, idx, default):
    return (xs[idx:idx+1]+[default])[0]


split_on = re.compile(get(sys.argv, 1, '\s+'))

last_words = None
for line in sys.stdin:
    words = split_on.split(line)
    if last_words:
        for word, last_word in zip_longest(words, last_words):
            if not word:
                break
            elif word != last_word:
                print(word, end='')
            else:
                print(' ' * len(word), end='')
        print()
    else:
        print(line)
    last_words = words
